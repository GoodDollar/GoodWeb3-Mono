{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "sourceMap": false,
    "removeComments": true
  },
  "include": ["src/**/*", "src/**/*.json"],
  "exclude": [
    "node_modules",
    "src/stories",
    "**/*.stories.tsx",
    "src/types",
    "src/__tests__",
    "src/locales/gen_supported_locales.ts",
    "__tests__"
  ]
}
